{
  "compilerOptions": {
    "sourceMap": true,
    "inlineSources": true,
    "noImplicitAny": false,
    "module": "es6",
    "target": "es5",
    "moduleResolution": "Node",
    "alwaysStrict": true,
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "AISKU/types",
    "outDir": "dist-esm",
    "rootDir": "AISKU/src"
  },
  "include": [
    "./src/**"
  ],
  "exclude": [
    "node/modules"
  ]
}